---
import Social from './Social.astro'
import { socialLinks } from './data.js'
---

<style>
  footer {
    display: flex;
    gap: 1rem;
    margin-top: 2rem;
  }
</style>

<footer>
  <ul>
    {
      socialLinks.map((link) => {
        return <Social key={link.id} {...link} />
      })
    }
  </ul>
</footer>
